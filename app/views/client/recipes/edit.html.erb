<%= form_tag "/client/recipes/#{ @recipe['id']}" method: :patch do %>
<div>
    Title: <input type="text" name="title">
  </div>
  <div>
    Chef: <input type="text" name="chef"<% @recipe['chef"] %>>
  </div>
  <div>
    Prep Time: <input type="text" name="prep_time"<% @recipe['chef"] %>>
  </div>
  <div>
    Ingredients: <input type="text" name="ingredients">
  </div>
  <div>
    Directions: <input type="text" name="directions">
  </div>
  <div>
    Image URL: <input type="text" name="image_url">
  </div>
  <input type="submit" value="Create Recipe">
<% end %>